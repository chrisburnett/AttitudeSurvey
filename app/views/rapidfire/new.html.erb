<script type="text/javascript">
 card_id = '<%= @card.id %>';
 <% if not @sensitivity_question %>
 recipient_id = <%= @recipient.id %>;
 window.survey_responses_path = '<%= survey_responses_path %>';
 <% end %>
</script>



<!-- which actual section gets rendered depends on the random
choice of response type -->

<% if @sensitivity_question %>
    <%= render 'sensitivity_question' %>
    <%= render 'sensitivity_results' %>
<% else %>
    <%= render 'recipient_question' %>
    <%= render 'recipient_results' %>
<% end %>
